# vim plugin manager
[submodule "pathogen"]
    path = pathogen
    url = git://github.com/tpope/vim-pathogen.git
# autocomplete with tab
[submodule "bundle/supertab"]
    path = bundle/supertab
    url = git://github.com/ervandew/supertab.git
# command-t like mate
[submodule "bundle/command-t"]
    path = bundle/command-t
    url = git://git.wincent.com/command-t.git
# nerdtree is nerdtree
[submodule "bundle/nerdtree"]
    path = bundle/nerdtree
    url = git://github.com/scrooloose/nerdtree.git
# easy way show file's buffer
[submodule "bundle/bufexplorer"]
    path = bundle/bufexplorer
    url = git://github.com/corntrace/bufexplorer.git
    ignore = dirty
# show a line with file details
[submodule "bundle/vim-powerline"]
    path = bundle/vim-powerline
    url = git://github.com/Lokaltog/vim-powerline.git
# automatic closing of quotes, parenthesis, brackets, etc
[submodule "bundle/delimitMate"]
    path = bundle/delimitMate
    url = git://github.com/Raimondi/delimitMate.git
# snippets like textmate behavior
[submodule "bundle/snipmate"]
    path = bundle/snipmate
    url = git://github.com/msanders/snipmate.vim.git
    ignore = dirty
# plugin that end certain structures automatically
[submodule "bundle/endwise"]
    path = bundle/endwise
    url = git://github.com/tpope/vim-endwise.git
# repeat last command with '.'
[submodule "bundle/repeat"]
    path = bundle/repeat
    url = https://github.com/tpope/vim-repeat
# work with rails easy
[submodule "bundle/vim-rails"]
    path = bundle/vim-rails
    url = https://github.com/tpope/vim-rails
# syntax highligh puppet scripts
[submodule "bundle/vim-puppet"]
    path = bundle/vim-puppet
    url = git://github.com/rodjek/vim-puppet.git
